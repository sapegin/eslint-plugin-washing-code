{
  "name": "eslint-plugin-washing-code",
  "description": "ESLint rules inspired by Washing your code book",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:vitest",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix",
    "test:vitest": "vitest run",
    "test:watch": "vitest",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"**/*.{js,mjc,cjs,ts,tsx,json,jsonc,md}\"",
    "prepublish": "npm run build",
    "prepare": "husky"
  },
  "homepage": "https://sapegin.me/book/",
  "author": {
    "name": "Artem Sapegin",
    "url": "https://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapegin/eslint-plugin-washing-code.git"
  },
  "bugs": {
    "url": "https://github.com/sapegin/eslint-plugin-washing-code/issues"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "lint",
    "clean code",
    "lint rule",
    "eslint rule"
  ],
  "license": "MIT",
  "type": "module",
  "exports": "./out/index.js",
  "types": "./out/index.d.ts",
  "files": [
    "out"
  ],
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/node": "^24.7.1",
    "@typescript-eslint/rule-tester": "8.46.0",
    "@typescript-eslint/utils": "8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-tamia": "^9.4.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,mjc,cjs,ts,tsx}": "eslint --cache --fix",
    "*.{js,mjc,cjs,ts,tsx,json,jsonc,md}": "prettier --write"
  }
}
